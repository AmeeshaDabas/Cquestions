/
#include <stdio.h>
#include<math.h>
int find_msd(int n)
{
    int k;
    k=(int)(log2(n));
    return pow(2,k);
}
int main()
{
    int n;
    printf("Enter the value of n ");
    scanf("%d",&n);
    int num;
    num=find_msd(n);
    printf("The most significant set bit is %d",num);
    return 0;
}
