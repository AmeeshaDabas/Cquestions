#include <stdio.h>

int main() {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    char str[1000000];
	    int i,c=0;
	    scanf("%s",str);
	    for(i=(strlen(str)-1);i>=0;i--)
	    {
	        if(str[i]=='1')
	        {
	            printf("%d\n",i);
	            c++;
	            break;
	        }
	    }
	    if(c==0)
	    {
	        printf("-1\n");
	    }
	}
	return 0;
}
