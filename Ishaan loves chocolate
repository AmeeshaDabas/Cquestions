#include <stdio.h>

int main() {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n,i;
	    scanf("%d",&n);
	    int arr[n];
	    for(i=0;i<n;i++)
	    {
	        scanf("%d",&arr[i]);
	    }
	    i=0;
	    int j=n-1;
	    while(i<=j)
	    {
	        if(arr[i]>arr[j])
	        {
	            i++;
	        }
	        else if(arr[j]>arr[i])
	        {
	            j--;
	        }
	        else if(i==j)
	        {
	            printf("%d\n",arr[i]);
	            break;
	        }
	    }
	   
	   
	}
	return 0;
}
