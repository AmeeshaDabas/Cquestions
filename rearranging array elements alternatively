#include <stdio.h>

int main() {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n,temp;
	    scanf("%d",&n);
	    int arr[n],i,j;
	    for(i=0;i<n;i++)
	    {
	        scanf("%d",&arr[i]);
	    }
        
       for(i=n-1,j=0;i>=j;i--,j++)
       {
           if(i==j)
           {
               printf("%d",arr[i]);
           }
           else
           printf("%d %d ",arr[i],arr[j]);
       }
       printf("\n");
	}
	return 0;
}
