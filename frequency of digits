#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char ch;
    int* arr=(int*) calloc(10,sizeof(int));
    while(scanf("%ch",&ch)==1)
    {
        if(ch>='0'&& ch<='9')
        {(*(arr+(ch-'0')))++;
        }
    }
    for(int i=0;i<10;i++)
    printf("%d ",*(arr+i)); 
    return 0;
}

