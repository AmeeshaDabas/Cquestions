
#include <stdio.h>
int calculate_xor(int n)
{
 switch(n%4)
    {
    case 0: return n;
    case 1: return 1;
    case 2: return n+1;
    case 3: return 0;
    }   
}
int main() {
    int n;
    //program to calculate xor of numbers from 1 to n
    printf("Enter the value of n ");
    scanf("%d",&n);
    int a;
    a=calculate_xor(n);
    printf("The xor of numbers from 1 to %d is %d ",n,a);
    
    
    return 0;
}
